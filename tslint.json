{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-config-standard",
    "tslint-config-airbnb",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "spaces", 2],
    "eofline": true,
    "prefer-const": true,
    "object-literal-sort-keys": [true, "shorthand-first"],
    "no-console": { "severity": "warning" }
  },
  "rulesDirectory": []
}
